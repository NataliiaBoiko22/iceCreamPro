{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "scale"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]",
          "iframe[allow]",
          "iframe[loading]"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}